          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Urahost",
          "href": "https://cp.urahost.fr",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Discord",
          "href": "https://discord.gg/urahost",
          "icon": "discord"
        },
        {
          "anchor": "Blog",
          "href": "https://bastien-b.fr",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.png",
    "dark": "/logo/dark.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "https://discord.gg/urahost"
      }
    ],
    "primary": {
      "type": "button",
      "label": "UraCad",
      "href": "https://uracad.com"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/Urahost"
    }
  }
